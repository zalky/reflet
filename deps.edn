{:deps    {com.fzakaria/slf4j-timbre {:mvn/version "0.3.20"}
           com.taoensso/timbre       {:mvn/version "4.10.0"}
           io.zalky/cinch            {:mvn/version "0.1.0"}
           org.clojure/clojure       {:mvn/version "1.10.3"}
           re-frame/re-frame         {:mvn/version "1.1.2"}
           reagent/reagent           {:mvn/version "1.0.0"}}
 :paths   ["src"]
 :aliases {:cljs        {:extra-paths ["client" "test"]
                         :extra-deps  {binaryage/devtools            {:mvn/version "1.0.3"}
                                       day8.re-frame/re-frame-10x    {:mvn/version "1.0.2"}
                                       day8.re-frame/test            {:mvn/version "0.1.5"}
                                       nrepl/nrepl                   {:mvn/version "0.8.3"}
                                       cider/cider-nrepl             {:mvn/version "0.28.5"}
                                       refactor-nrepl/refactor-nrepl {:mvn/version "2.5.1"}
                                       thheller/shadow-cljs          {:mvn/version "2.16.10"}}
                         :main-opts   ["-m" "shadow.cljs.devtools.cli"]}
           :cljs/report {:main-opts ["-m" "shadow.cljs.build-report" "app" "target/report.html"]}
           :sass        {:extra-deps {io.zalky/dart-sass-clj {:mvn/version "0.2.0"}}
                         :main-opts  ["-m" "dart-sass-clj.core"
                                      "--source-dirs" "[\"client/scss/\"]"
                                      "--target-dir" "resources"
                                      "--output-style" ":expanded"
                                      "--source-maps"
                                      "--watch"]}
           :build       {:deps       {io.zalky/build-clj {:git/url "https://github.com/zalky/build-clj.git"
                                                          :git/sha "b38281eda585731a7ba330e3d66ba2d8c74e4540"}}
                         :ns-default io.zalky.build}}}
